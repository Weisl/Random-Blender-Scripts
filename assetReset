import bpy

selection = bpy.context.selected_objects.copy()
properties = {
    "assetID",
    "subAssetIDs",
    "masterAssetID",
    "masterAsset",
    "assetState",
    "validName",
    "location",
    "category",
    "type",
    "assetState",
    "basename",
}

for obj in selection: 
    for property in properties:
        if obj.get(property) is not None:
            obj[property] = None
        